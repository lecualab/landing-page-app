{
  "name": "@lecualab/landing-page",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:docker": "docker compose -f compose.yml -f compose.dev.yml up --build -d",
    "stop:docker": "docker compose -f compose.yml -f compose.dev.yml down",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:ci": "pnpm test --no-watch --no-progress --browsers=ChromeHeadless",
    "analyze-bundle": "ng build --configuration=analyze-bundle && source-map-explorer dist/**/*.js --no-border-checks",
    "lint": "ng lint",
    "lint:types": "tsc --noEmit",
    "format:check": "prettier --check",
    "format:all": "pnpm format:check --write .",
    "prepare": "husky"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^21.2.0",
    "@angular/cdk": "21.2.0",
    "@angular/common": "^21.2.0",
    "@angular/compiler": "^21.2.0",
    "@angular/core": "^21.2.0",
    "@angular/forms": "^21.2.0",
    "@angular/material": "21.2.0",
    "@angular/platform-browser": "^21.2.0",
    "@angular/platform-browser-dynamic": "^21.2.0",
    "@angular/platform-server": "^21.2.0",
    "@angular/router": "^21.2.0",
    "@angular/ssr": "^21.2.0",
    "@fortawesome/angular-fontawesome": "4.0.0",
    "@fortawesome/free-brands-svg-icons": "^7.2.0",
    "@netlify/angular-runtime": "^3.0.1",
    "@netlify/plugin-sitemap": "^0.8.1",
    "@ngx-translate/core": "^17.0.0",
    "@ngx-translate/http-loader": "^17.0.0",
