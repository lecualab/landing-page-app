    "cookieconsent": "^3.1.1",
    "express": "^5.2.1",
    "ngx-cookieconsent": "^8.0.0",
    "ngx-multi-pixel": "^2.2.2",
    "ngx-skeleton-loader": "^12.0.0",
    "ngx-translate-cut": "^21.1.0",
    "phone": "^3.1.71",
    "rxjs": "~7.8.2",
    "tslib": "^2.8.1"
  },
