    "@types/express": "^5.0.6",
    "@types/jasmine": "~5.1.12",
    "@types/node": "^25.3.2",
    "@types/testing-library__jasmine-dom": "^1.3.4",
    "angular-eslint": "21.2.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
