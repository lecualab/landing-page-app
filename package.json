{
  "name": "@lecualab/landing-page",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:docker": "docker compose -f compose.yml -f compose.dev.yml up --build -d",
    "stop:docker": "docker compose -f compose.yml -f compose.dev.yml down",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:ci": "pnpm test -- --no-watch --no-progress --browsers=ChromeHeadless",
    "serve:ssr": "node dist/lecualab/landing-page/server/server.mjs",
    "analyze-bundle": "ng build --configuration=analyze-bundle && source-map-explorer dist/**/*.js --no-border-checks",
    "lint": "ng lint",
    "lint:types": "tsc --noEmit",
    "format:check": "prettier --check",
    "format:all": "pnpm format:check --write .",
    "prepare": "husky"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.2.1",
    "@angular/cdk": "19.2.2",
    "@angular/common": "^19.2.1",
    "@angular/compiler": "^19.2.1",
    "@angular/core": "^19.2.1",
    "@angular/forms": "^19.2.1",
    "@angular/material": "19.2.2",
    "@angular/platform-browser": "^19.2.1",
    "@angular/platform-browser-dynamic": "^19.2.1",
    "@angular/platform-server": "^19.2.1",
    "@angular/router": "^19.2.1",
    "@angular/ssr": "^19.2.1",
    "@ngx-translate/core": "^16.0.4",
    "@ngx-translate/http-loader": "^16.0.1",
    "cache-manager": "^6.4.1",
    "express": "^5.0.1",
    "ngx-translate-cut": "^19.0.2",
    "rxjs": "~7.8.2",
    "tslib": "^2.8.1",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.1",
    "@angular/cli": "^19.2.1",
    "@angular/compiler-cli": "^19.2.1",
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-angular": "^19.8.0",
    "@eslint/js": "^9.22.0",
    "@smarttools/eslint-plugin-rxjs": "^1.0.18",
    "@stylistic/eslint-plugin": "^4.2.0",
    "@tailwindcss/postcss": "^4.0.12",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/angular": "17.3.6",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jasmine-dom": "^1.3.3",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/user-event": "^14.6.1",
    "@types/express": "^5.0.0",
    "@types/jasmine": "~5.1.7",
    "@types/node": "^22.13.10",
    "@types/testing-library__jasmine-dom": "^1.3.4",
    "angular-eslint": "19.2.1",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-jasmine": "^4.2.2",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-sonarjs": "^3.0.2",
    "eslint-plugin-testing-library": "^7.1.1",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "jasmine-core": "~5.6.0",
    "karma": "~6.4.4",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "lint-staged": "^15.4.3",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "prettier-eslint": "^16.3.0",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "source-map-explorer": "^2.5.3",
    "tailwindcss": "^4.0.12",
    "typescript": "~5.8.2",
    "typescript-eslint": "8.26.1"
  },
  "packageManager": "pnpm@10.6.2"
}
